{
  "name": "serve",
  "version": "11.4.0",
  "description": "Static file serving and directory listing",
  "scripts": {
    "build": "rimraf dist && webpack --progress",
    "test": "yarn lint",
    "lint": "zeit-eslint --ext .jsx,.js .",
    "lint-staged": "git diff --diff-filter=ACMRT --cached --name-only '*.js' '*.jsx' | xargs zeit-eslint"
  },
  "files": [
    "bin"
  ],
  "repository": "zeit/serve",
  "bin": {
    "serve": "./bin/serve.js"
  },
  "keywords": [
    "now",
    "serve",
    "micro",
    "http-server"
  ],
  "author": "leo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.4",
    "@zeit/eslint-config-node": "0.3.0",
    "@zeit/git-hooks": "0.1.4",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.0.5",
    "eslint": "5.4.0",
    "string-replace-loader": "2.2.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.2.2"
  },
  "eslintConfig": {
    "extends": [
      "@zeit/eslint-config-node"
    ]
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@zeit/schemas": "2.6.0",
    "ajv": "6.5.3",
    "arg": "2.0.0",
    "boxen": "1.3.0",
    "chalk": "2.4.1",
    "clipboardy": "1.2.3",
    "compression": "1.7.3",
    "serve-handler": "6.1.3",
    "update-check": "1.5.2"
  },
  "resolutions": {
    "**/assert/util": "^0.11.0",
    "**/babel-code-frame/chalk": "^2.1.0",
    "**/cliui/wrap-ansi": "~3.0.0",
    "**/debug/ms": "^2.1.1",
    "**/define-property/is-descriptor": "^1.0.0",
    "boxen/term-size/execa": "^0.8.0",
    "serve-handler/mime-types": "~2.1.24"
  },
  "engines": {
    "node": ">=6"
  }
}
